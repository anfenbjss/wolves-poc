{
    "type": "module",
    "private": true,
    "homepage": "https://github.com/anfenbjss/wolves-poc",
    "scripts": {
        "build": "tsc -b && vite build",
        "dev": "vite",
        "lint": "eslint . --max-warnings=0 --fix \"src/**/*.{js,jsx,ts,tsx,json}\"",
        "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
        "preview": "npm run build && vite preview",
        "cap-sync": "npm run build && npx cap sync",
        "todo": "npm run build && npx cap run --target R58MB508HDR android",
        "android-list": "npx cap run --list android",
        "open-android": "npx cap open android",
        "test-unit": "vitest ./src --run",
        "test-e2e": "TF_BUILD=true playwright test",
        "test-e2e-ui": "TF_BUILD=true playwright test --ui",
        "deploy": "npm run build && gh-pages -d dist"
    },
    "dependencies": {
        "@capacitor/android": "^7.2.0",
        "@capacitor/cli": "^7.2.0",
        "@capacitor/core": "^7.2.0",
        "@microsoft/applicationinsights-web": "^3.3.6",
        "react": "^19.0.0",
        "react-aria-components": "^1.8.0",
        "react-dom": "^19.0.0",
        "xlsx": "^0.18.5"
    },
    "devDependencies": {
        "@eslint/js": "^9.21.0",
        "@playwright/test": "^1.50.1",
        "@testing-library/dom": "^10.4.0",
        "@testing-library/react": "^16.3.0",
        "@trivago/prettier-plugin-sort-imports": "^5.2.2",
        "@types/node": "^22.13.8",
        "@types/react": "^19.1.2",
        "@types/react-dom": "^19.1.2",
        "@vitejs/plugin-basic-ssl": "^2.0.0",
        "@vitejs/plugin-react": "^4.3.4",
        "eslint": "^9.21.0",
        "eslint-plugin-react-hooks": "^5.0.0",
        "eslint-plugin-react-refresh": "^0.4.19",
        "gh-pages": "^6.3.0",
        "globals": "^16.0.0",
        "jsdom": "^26.1.0",
        "prettier": "^3.5.2",
        "typescript": "^5.7.2",
        "typescript-eslint": "^8.24.1",
        "vite": "^6.2.0",
        "vite-plugin-pwa": "^1.0.0",
        "vite-plugin-qrcode": "^0.2.4",
        "vitest": "^3.0.7"
    }
}
